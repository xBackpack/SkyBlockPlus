{
	"schemaVersion": 1,
	"id": "skyblockplus",
	"version": "${version}",
	"name": "SkyBlockPlus",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/skyblockplus/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "me.xbackpack.skyblockplus.SkyBlockPlus",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "me.xbackpack.skyblockplus.SkyBlockPlusClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "me.xbackpack.skyblockplus.SkyBlockPlusDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"skyblockplus.mixins.json",
		{
			"config": "skyblockplus.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.13",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}